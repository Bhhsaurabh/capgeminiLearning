package OperatorsAss;

public class problem10 {
	public static void main(String[] args) {
		int score = 200;
		int bonous = 5;
		boolean inTime  = true;
		boolean isCheated = true;
		
		boolean getBonous = !isCheated && (score > 100 && inTime)?true: false;
		score = getBonous ? score += bonous : score;
		
		System.out.println("Total Score : " + score); 
	}
}
