package OperatorsAss;

public class problem4 {
	public static void main(String[] args) {
		int dic = 100;
		int totalPur = 3000;
		int totalPrice = 0;
		boolean isMem = true;
		int noOfTimeVis = 4;
		boolean isELi = noOfTimeVis > 1 || isMem && totalPur >= 2000 ? true : false;
		totalPrice = isELi ? TotalPur -= dic  : totalPur;
	}
}
